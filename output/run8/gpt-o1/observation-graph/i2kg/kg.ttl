@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-action#> .
@prefix ex:     <http://example.org/data/> .

ex:Kitchen a psr:Environment ;
    dul:hasComponent ex:Fridge ;
    dul:hasComponent ex:Cabinet ;
    dul:hasComponent ex:Counter ;
    dul:hasComponent ex:Sink ;
    dul:hasComponent ex:Stove ;
    dul:hasComponent ex:Table ;
    dul:hasComponent ex:Chair1 ;
    dul:hasComponent ex:Chair2 ;
    dul:hasComponent ex:Chair3 ;
    dul:hasComponent ex:Chair4 ;
    dul:hasComponent ex:Jam ;
    dul:hasComponent ex:Salt ;
    dul:hasComponent ex:Jello ;
    dul:hasComponent ex:CerealBox1 ;
    dul:hasComponent ex:CerealBox2 ;
    dul:hasComponent ex:Plates ;
    dul:hasComponent ex:Mug ;
    dul:hasComponent ex:Pot ;
    dul:hasComponent ex:WaterBottle ;
    dul:hasComponent ex:FruitBowl .

ex:Fridge a psr:Appliance ;
    psr:hasAffordance ex:OpenFridge,
                      ex:CloseFridge .

ex:OpenFridge a soma:Opening .
ex:CloseFridge a soma:Closing .

ex:Cabinet a psr:Furniture .
ex:Counter a psr:Furniture .
ex:Sink a psr:Appliance .
ex:Stove a psr:Appliance .
ex:Table a psr:Furniture .

ex:Chair1 a psr:Furniture .
ex:Chair2 a psr:Furniture .
ex:Chair3 a psr:Furniture .
ex:Chair4 a psr:Furniture .

ex:Jam a psr:Object ;
    psr:hasAffordance ex:PickUpJam,
                      ex:PutDownJam ;
    dul:hasLocation ex:JamLocation .

ex:PickUpJam a soma:PickingUp .
ex:PutDownJam a soma:PuttingDown .

ex:JamLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:Salt a psr:Object ;
    dul:hasLocation ex:SaltLocation .

ex:SaltLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:Jello a psr:Object ;
    dul:hasLocation ex:JelloLocation .

ex:JelloLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:CerealBox1 a psr:Object ;
    dul:hasLocation ex:CerealBox1Location .

ex:CerealBox1Location a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:CerealBox2 a psr:Object ;
    dul:hasLocation ex:CerealBox2Location .

ex:CerealBox2Location a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:Plates a psr:Object ;
    dul:hasLocation ex:PlatesLocation .

ex:PlatesLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:Mug a psr:Object ;
    dul:hasLocation ex:MugLocation .

ex:MugLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:Pot a psr:Object ;
    psr:hasAffordance ex:PickUpPot,
                      ex:PutDownPot ;
    dul:hasLocation ex:PotLocation .

ex:PickUpPot a soma:PickingUp .
ex:PutDownPot a soma:PuttingDown .

ex:PotLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Stove .

ex:WaterBottle a psr:Object ;
    dul:hasLocation ex:WaterBottleLocation .

ex:WaterBottleLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Table .

ex:FruitBowl a psr:Object ;
    dul:hasLocation ex:FruitBowlLocation .

ex:FruitBowlLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Table .

ex:Chair1Location a psr:CurrentLocation ;
    geo:sfWithin ex:Kitchen .

ex:Chair2Location a psr:CurrentLocation ;
    geo:sfWithin ex:Kitchen .

ex:Chair3Location a psr:CurrentLocation ;
    geo:sfWithin ex:Kitchen .

ex:Chair4Location a psr:CurrentLocation ;
    geo:sfWithin ex:Kitchen .

ex:Chair1 dul:hasLocation ex:Chair1Location .
ex:Chair2 dul:hasLocation ex:Chair2Location .
ex:Chair3 dul:hasLocation ex:Chair3Location .
ex:Chair4 dul:hasLocation ex:Chair4Location .