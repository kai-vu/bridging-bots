@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-action#> .
@prefix ex:     <http://example.org/data/> .

ex:kitchen a psr:Environment .
ex:countertop a psr:Component ;
    dul:hasLocation ex:kitchen_location .
ex:kitchen_location a psr:CurrentLocation ;
    psr:onTopOf ex:sink .
ex:sink a psr:Component ;
    dul:hasLocation ex:kitchen_location .
ex:cabinets a psr:Component ;
    dul:hasLocation ex:kitchen_location .
ex:oven a psr:Appliance ;
    dul:hasLocation ex:kitchen_location .
ex:refrigerator a psr:Appliance ;
    dul:hasLocation ex:kitchen_location .
ex:dining_table a psr:Furniture ;
    dul:hasLocation ex:dining_area_location .
ex:dining_area_location a psr:CurrentLocation .
ex:chairs a psr:Furniture ;
    dul:hasLocation ex:dining_area_location .
ex:floor a psr:Component ;
    dul:hasLocation ex:kitchen_location .
ex:ceiling_light a psr:Component ;
    dul:hasLocation ex:kitchen_location .
ex:paper_towels a psr:Object ;
    dul:hasLocation ex:kitchen_location .
ex:bottle a psr:Object ;
    dul:hasLocation ex:kitchen_location .
ex:boxes a psr:Object ;
    dul:hasLocation ex:kitchen_location .
ex:books a psr:Object ;
    dul:hasLocation ex:dining_area_location .
ex:red_bottle a psr:Object ;
    dul:hasLocation ex:dining_area_location .

ex:kitchen dul:hasComponent ex:countertop .
ex:kitchen dul:hasComponent ex:sink .
ex:kitchen dul:hasComponent ex:cabinets .
ex:kitchen dul:hasComponent ex:oven .
ex:kitchen dul:hasComponent ex:refrigerator .
ex:kitchen dul:hasComponent ex:dining_table .
ex:kitchen dul:hasComponent ex:chairs .
ex:kitchen dul:hasComponent ex:floor .
ex:kitchen dul:hasComponent ex:ceiling_light .
ex:kitchen dul:hasComponent ex:paper_towels .
ex:kitchen dul:hasComponent ex:bottle .
ex:kitchen dul:hasComponent ex:boxes .
ex:kitchen dul:hasComponent ex:books .
ex:kitchen dul:hasComponent ex:red_bottle .

ex:sink psr:hasAffordance soma:Closing .
ex:sink psr:hasAffordance soma:Opening .
ex:oven psr:hasAffordance soma:Opening .
ex:oven psr:hasAffordance soma:Closing .
ex:refrigerator psr:hasAffordance soma:Opening .
ex:refrigerator psr:hasAffordance soma:Closing .
ex:dining_table psr:hasAffordance soma:Pulling .
ex:dining_table psr:hasAffordance soma:Pushing .
ex:chairs psr:hasAffordance soma:Pulling .
ex:chairs psr:hasAffordance soma:Pushing .
ex:boxes psr:hasAffordance soma:PickingUp .
ex:boxes psr:hasAffordance soma:PuttingDown .
ex:bottle psr:hasAffordance soma:Grasping .
ex:bottle psr:hasAffordance soma: Holding .
ex:paper_towels psr:hasAffordance soma:Grasping .
ex:paper_towels psr:hasAffordance soma: Holding .
ex:books psr:hasAffordance soma:PickingUp .
ex:books psr:hasAffordance soma:PuttingDown .
ex:red_bottle psr:hasAffordance soma:Grasping .
ex:red_bottle psr:hasAffordance soma: Holding .

ex:kitchen_location geo:sfContains ex:sink .
ex:kitchen_location geo:sfContains ex:cabinets .
ex:kitchen_location geo:sfContains ex:oven .
ex:kitchen_location geo:sfContains ex:refrigerator .
ex:kitchen_location geo:sfContains ex:countertop .
ex:dining_area_location geo:sfContains ex:dining_table .
ex:dining_area_location geo:sfContains ex:chairs .