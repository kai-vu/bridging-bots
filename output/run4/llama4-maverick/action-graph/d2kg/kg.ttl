    psr:actsOn ex:BoxesOfFood ;
    psr:isAffordedBy soma:PuttingDown ;
    psr:follows ex:IdentifyMisplacedItems_Action1 .

ex:BoxesOfFood a psr:Object ;
    rdfs:label "Boxes of food"@en .

