@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/OntoBOT#> .
@prefix ex: <http://example.org/data/> .

ex:Environment1 a psr:Environment ;
    dul:hasComponent ex:KitchenCabinet1, ex:Refrigerator1, ex:DiningTable1, ex:Chair1, ex:Chair2, ex:Chair3, ex:Sink1, ex:CookingAppliance1 .

ex:KitchenCabinet1 a psr:Furniture ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation ex:KitchenCounter1 .

ex:KitchenCounter1 a psr:Location ;
    geo:sfContains ex:Jar1, ex:Jello1, ex:Salt1, ex:Box1, ex:Glass1, ex:Pot1, ex:Book1 .

ex:Jar1 a psr:Object ;
    dul:hasLocation ex:KitchenCounter1 .

ex:Jello1 a psr:Object ;
    dul:hasLocation ex:KitchenCounter1 .

ex:Salt1 a psr:Object ;
    dul:hasLocation ex:KitchenCounter1 .

ex:Refrigerator1 a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:DiningTable1 a psr:Furniture ;
    dul:hasLocation ex:DiningRoom1 ;
    geo:sfContains ex:WaterBottle1, ex:Mug1, ex:Book2 .

ex:DiningRoom1 a psr:Location .

ex:Chair1 a psr:Furniture ;
    dul:hasLocation ex:DiningRoom1 .

ex:Chair2 a psr:Furniture ;
    dul:hasLocation ex:DiningRoom1 .

ex:Chair3 a psr:Furniture ;
    dul:hasLocation ex:DiningRoom1 .

ex:Sink1 a psr:Furniture ;
    dul:hasLocation ex:KitchenCounter1 .

ex:CookingAppliance1 a psr:Appliance ;
    dul:hasLocation ex:KitchenCounter1 .

ex:WaterBottle1 a psr:Object ;
    dul:hasLocation ex:DiningTable1 .

ex:Mug1 a psr:Object ;
    dul:hasLocation ex:DiningTable1 .

ex:Book1 a psr:Object ;
    dul:hasLocation ex:KitchenCounter1 .

ex:Book2 a psr:Object ;
    dul:hasLocation ex:DiningTable1 .

ex:Pot1 a psr:Object ;
    dul:hasLocation ex:KitchenCounter1 .

ex:Glass1 a psr:Object ;
    dul:hasLocation ex:KitchenCounter1 .

ex:Box1 a psr:Object ;
    dul:hasLocation ex:KitchenCounter1 .