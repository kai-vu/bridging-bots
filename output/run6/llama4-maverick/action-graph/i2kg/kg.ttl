    soma:isPerformedBy dul:Agent_1 ;
    psr:precedes psr:Action_11 .

psr:Action_11 a dul:Action ;
    rdfs:label "Pick up the pack of plates"@en ;
    psr:actsOn psr:Object_6 ;
    psr:isAffordedBy soma:PickingUp ;
    soma:isPerformedBy dul:Agent_1 ;
    psr:precedes psr:Action_12 .

psr:Action_12 a dul:Action ;
    rdfs:label "Put down the pack of plates in the cupboard"@en ;
    psr:actsOn psr:Object_6 ;
    psr:isAffordedBy soma:PuttingDown ;
    soma:isPerformedBy dul:Agent_1 .

dul:Agent_1 a dul:Agent .

psr:Object_1 a psr:Object ;
    rdfs:label "Box of 'Chucky'"@en .

psr:Object_2 a psr:Object ;
    rdfs:label "Jar of orange juice"@en .

psr:Object_3 a psr:Object ;
    rdfs:label "Jar of 'Marmalade'"@en .

psr:Object_4 a psr:Object ;
    rdfs:label "Box of 'Jello'"@en .

psr:Object_5 a psr:Object ;
    rdfs:label "Box of 'Alpen Salt'"@en .

psr:Object_6 a psr:Object ;
    rdfs:label "Pack of plates"@en .