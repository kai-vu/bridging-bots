@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix ex: <http://example.org/data/> .

ex:RestoreKitchenTask a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en ;
    psr:hasWorkflow ex:RestoreKitchenWorkflow .

ex:RestoreKitchenWorkflow a psr:Workflow ;
    psr:hasAction ex:IdentifyMisplacedItemsAction ;
    psr:hasAction ex:PutAwayItemsAction .

ex:IdentifyMisplacedItemsAction a dul:Action ;
    psr:precedes ex:PutAwayItemsAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItems .

ex:PutAwayItemsAction a dul:Action ;
    psr:follows ex:IdentifyMisplacedItemsAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItems ;
    psr:isAffordedBy soma:PuttingDown .

ex:MisplacedItems a psr:Object .

ex:RobotAgent a dul:Agent .

ex:DiningTable a psr:Furniture .
ex:KitchenCounter a psr:Furniture .
ex:Refrigerator a psr:Appliance .
ex:Stove a psr:Appliance .
ex:Sink a psr:Object .
ex:Bottle a psr:Object .
ex:StackOfPlates a psr:Object .
ex:BoxOnTable a psr:Object .
ex:JarOnCounter a psr:Object .
ex:BoxOnCounter a psr:Object .
ex:ChickyBox a psr:Object .
ex:JelloBox a psr:Object .
ex:JamJar a psr:Object .
ex:PlatesOnCounter a psr:Object .
ex:CupsOnCounter a psr:Object .

ex:IdentifyMisplacedItemsAction psr:isAffordedBy soma:PickingUp .
ex:PutAwayItemsAction psr:precedes ex:OpenStorageAction .
ex:OpenStorageAction a dul:Action ;
    psr:follows ex:PutAwayItemsAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:Storage ;
    psr:isAffordedBy soma:Opening .

ex:PutItemsInStorageAction a dul:Action ;
    psr:follows ex:OpenStorageAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItems ;
    psr:isAffordedBy soma:PuttingDown .

ex:CloseStorageAction a dul:Action ;
    psr:follows ex:PutItemsInStorageAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:Storage ;
    psr:isAffordedBy soma:Closing .

ex:Storage a psr:Component .

ex:RestoreKitchenWorkflow psr:hasAction ex:OpenStorageAction .
ex:RestoreKitchenWorkflow psr:hasAction ex:PutItemsInStorageAction .
ex:RestoreKitchenWorkflow psr:hasAction ex:CloseStorageAction .

ex:IdentifyMisplacedItemsAction psr:actsOn ex:Bottle .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:StackOfPlates .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:BoxOnTable .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:JarOnCounter .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:BoxOnCounter .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:ChickyBox .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:JelloBox .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:JamJar .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:PlatesOnCounter .
ex:IdentifyMisplacedItemsAction psr:actsOn ex:CupsOnCounter .

ex:OpenStorageAction psr:actsOn ex:Cabinet .
ex:Cabinet a psr:Furniture .

ex:PutItemsInStorageAction psr:actsOn ex:Bottle .
ex:PutItemsInStorageAction psr:actsOn ex:StackOfPlates .
ex:PutItemsInStorageAction psr:actsOn ex:BoxOnTable .
ex:PutItemsInStorageAction psr:actsOn ex:JarOnCounter .
ex:PutItemsInStorageAction psr:actsOn ex:BoxOnCounter .
ex:PutItemsInStorageAction psr:actsOn ex:ChickyBox .
ex:PutItemsInStorageAction psr:actsOn ex:JelloBox .
ex:PutItemsInStorageAction psr:actsOn ex:JamJar .
ex:PutItemsInStorageAction psr:actsOn ex:PlatesOnCounter .
ex:PutItemsInStorageAction psr:actsOn ex:CupsOnCounter .

ex:CloseStorageAction psr:actsOn ex:Cabinet .