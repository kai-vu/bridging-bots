@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/onto-bot#> .
@prefix ex: <http://example.org/data/> .

ex:Restore_Kitchen a psr:Instruction ;
  psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en ;
  psr:hasWorkflow ex:Workflow_1 .

ex:Workflow_1 a psr:Workflow ;
  psr:hasAction ex:Identify_Misplaced_Items .

ex:Identify_Misplaced_Items a dul:Action ;
  psr:isPerformedBy ex:Robot ;
  psr:actsOn ex:Kitchen_Countertop ;
  psr:isAffordedBy soma:PickingUp .

ex:Kitchen_Countertop a psr:Component ;
  rdfs:label "Kitchen Countertop"@en .

ex:Robot a dul:Agent ;
  rdfs:label "Robot"@en .

ex:Workflow_1 psr:hasAction ex:Pick_Up_Item_1 .

ex:Pick_Up_Item_1 a dul:Action ;
  psr:isPerformedBy ex:Robot ;
  psr:actsOn ex:Jar_Of_Jam_1 ;
  psr:isAffordedBy soma:PickingUp ;
  psr:follows ex:Identify_Misplaced_Items .

ex:Jar_Of_Jam_1 a psr:Object ;
  rdfs:label "Jar of Jam 1"@en .

ex:Workflow_1 psr:hasAction ex:Put_Down_Item_1 .

ex:Put_Down_Item_1 a dul:Action ;
  psr:isPerformedBy ex:Robot ;
  psr:actsOn ex:Jar_Of_Jam_1 ;
  psr:isAffordedBy soma:PuttingDown ;
  psr:follows ex:Pick_Up_Item_1 .

ex:Workflow_1 psr:hasAction ex:Pick_Up_Item_2 .

ex:Pick_Up_Item_2 a dul:Action ;
