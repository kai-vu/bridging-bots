  psr:follows ex:Put_Down_Item_1 .

ex:Box_Of_Jell-O a psr:Object ;
  rdfs:label "Box of Jell-O"@en .

ex:Workflow_1 psr:hasAction ex:Put_Down_Item_2 .

ex:Put_Down_Item_2 a dul:Action ;
  psr:isPerformedBy ex:Robot ;
  psr:actsOn ex:Box_Of_Jell-O ;
  psr:isAffordedBy soma:PuttingDown ;
  psr:follows ex:Pick_Up_Item_2 .

