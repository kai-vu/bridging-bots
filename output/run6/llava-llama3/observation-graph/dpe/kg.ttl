@prefix kitchen: <http://example.org/kitchen#> .

kitchen:kitchen kitchen:hasComponent kitchen:cabinets,
        kitchen:countertop,
        kitchen:dining_table,
        kitchen:island,
        kitchen:refrigerator,
        kitchen:sink,
        kitchen:walls,
        kitchen:window .

kitchen:cabinets kitchen:hasAffordance kitchen:storage_space .

kitchen:countertop kitchen:hasComponent kitchen:box_of_cereal,
        kitchen:tin_can .

kitchen:dining_table kitchen:hasComponent kitchen:chairs .

kitchen:island kitchen:hasComponent kitchen:cups,
        kitchen:pot_of_coffee,
        kitchen:stove_top .

kitchen:refrigerator kitchen:hasComponent kitchen:food_items,
        kitchen:microwave .

kitchen:sink kitchen:hasComponent kitchen:faucet .

kitchen:walls kitchen:hasComponent kitchen:picture_frame .

