@prefix owl: <http://www.w3.org/2002/07/owl#>
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
@prefix psr: <http://example.org/data/>

:environment a owl:Class ;
    rdfs:label "Environment"@en ;
    owl:intersectionOf ( :room ;
                            owl:hasComponent * :component ) .

:room a owl:Class ;
    rdfs:label "Room"@en .

:furniture a owl:Class ;
    rdfs:label "Furniture"@en .

:appliance a owl:Class ;
    rdfs:label "Appliance"@en .

:object a owl:Class ;
    rdfs:label "Object"@en .

:component a owl:Class ;
    rdfs:label "Component"@en .

:affordance a owl:Class ;
    rdfs:label "Affordance"@en .

:location a owl:Class ;
    rdfs:label "Location"@en .

:standard-location a owl:Class ;
    rdfs:label "Standard Location"@en .

:current-location a owl:Class ;
    rdfs:label "Current Location"@en .

:action a owl:Class ;
    rdfs:label "Action"@en .

:closing a psr:Affordance ;
    rdfs:label "Closing"@en .

:opening a psr:Affordance ;
    rdfs:label "Opening"@en .

:holding a psr:Affordance ;
    rdfs:label "Holding"@en .

:picking-up a psr:Affordance ;
    rdfs:label "Picking Up"@en .

:putting-down a psr:Affordance ;
    rdfs:label "Putting Down"@en .

:pulling a psr:Affordance ;
    rdfs:label "Pulling"@en .

:grasping a psr:Affordance ;
    rdfs:label "Grasping"@en .

:on-top-of a psr:Affordance ;
    rdfs:label "On Top Of"@en .

:contains a psr:Affordance ;
    rdfs:label "Contains"@en .

:is-within a psr:Affordance ;
    rdfs:label "Is Within"@en .

:overlaps a psr:Affordance ;
    rdfs:label "Overlaps"@en .

:component1 a :component ;
    owl:sameAs psr:Environment#component1 .

:component2 a :component ;
    owl:sameAs psr:Environment#component2 .

:object1 a :object ;
    owl:sameAs psr:Environment#object1 .

:object2 a :object ;
    owl:sameAs psr:Environment#object2 .

:furniture1 a :furniture ;
    owl:sameAs psr:Environment#furniture1 .

:appliance1 a :appliance ;
    owl:sameAs psr:Environment#appliance1 .

:location1 a :location ;
    owl:sameAs psr:Environment#location1 .

:current-location1 a :current-location ;
    owl:sameAs psr:Environment#current-location1 .

:standard-location1 a :standard-location ;
    owl:sameAs psr:Environment#standard-location1 .

:action1 a :action ;
    owl:sameAs psr:Environment#action1 .