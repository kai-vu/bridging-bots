@prefix ex: <http://example.org/data/> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ex:Kitchen a psr:Environment ;
    dul:hasComponent ex:Fridge,
                     ex:Stove,
                     ex:Counter,
                     ex:DiningTable,
                     ex:Chair1,
                     ex:Chair2,
                     ex:Chair3,
                     ex:Chair4,
                     ex:JamJar,
                     ex:JelloBox,
                     ex:SaltContainer,
                     ex:PlateStack,
                     ex:Pot,
                     ex:Mug,
                     ex:Pitcher,
                     ex:WaterBottle,
                     ex:FruitBowl,
                     ex:CerealBox1,
                     ex:CerealBox2,
                     ex:Door,
                     ex:Window .

ex:Fridge a psr:Appliance ;
    psr:hasAffordance soma:Opening,
                       soma:Closing .

ex:Stove a psr:Appliance .

ex:Counter a psr:Furniture .

ex:DiningTable a psr:Furniture .

ex:Chair1 a psr:Furniture .
ex:Chair2 a psr:Furniture .
ex:Chair3 a psr:Furniture .
ex:Chair4 a psr:Furniture .

ex:Door a psr:Object ;
    psr:hasAffordance soma:Opening,
                       soma:Closing .

ex:Window a psr:Object ;
    psr:hasAffordance soma:Opening,
                       soma:Closing .

ex:JamJar a psr:Object ;
    dul:hasLocation ex:LocJamJar .

ex:JelloBox a psr:Object ;
    dul:hasLocation ex:LocJelloBox .

ex:SaltContainer a psr:Object ;
    dul:hasLocation ex:LocSaltContainer .

ex:PlateStack a psr:Object ;
    dul:hasLocation ex:LocPlateStack .

ex:CerealBox1 a psr:Object ;
    dul:hasLocation ex:LocCerealBox1 .

ex:CerealBox2 a psr:Object ;
    dul:hasLocation ex:LocCerealBox2 .

ex:Pot a psr:Object ;
    psr:hasAffordance soma:Holding ;
    dul:hasLocation ex:LocPot .

ex:Mug a psr:Object ;
    psr:hasAffordance soma:Holding ;
    dul:hasLocation ex:LocMug .

ex:Pitcher a psr:Object ;
    psr:hasAffordance soma:Holding ;
    dul:hasLocation ex:LocPitcher .

ex:WaterBottle a psr:Object ;
    psr:hasAffordance soma:Holding,
                       soma:Delivering ;
    dul:hasLocation ex:LocWaterBottle .

ex:FruitBowl a psr:Object ;
    dul:hasLocation ex:LocFruitBowl .

ex:LocJamJar a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:LocJelloBox a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:LocSaltContainer a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:LocPlateStack a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:LocCerealBox1 a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:LocCerealBox2 a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:LocPot a psr:CurrentLocation ;
    psr:onTopOf ex:Stove .

ex:LocMug a psr:CurrentLocation ;
    psr:onTopOf ex:Counter .

ex:LocPitcher a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:LocWaterBottle a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:LocFruitBowl a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .