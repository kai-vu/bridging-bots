@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/onto-bot#> .
@prefix ex:     <http://example.org/data/> .

ex:Kitchen a psr:Environment ;
    dul:hasComponent ex:UpperCabinets,
                     ex:LowerCabinets,
                     ex:Countertop,
                     ex:Refrigerator,
                     ex:TallBlackCabinet,
                     ex:JamJar1,
                     ex:StrawberryJelloBox,
                     ex:SaltContainer,
                     ex:WhitePlatesStack,
                     ex:Sink,
                     ex:SmallMetalPot,
                     ex:VentHood,
                     ex:Stove,
                     ex:CerealBoxChucky,
                     ex:CerealBoxRiceBubbles,
                     ex:OrangeMug,
                     ex:JamJar2,
                     ex:DiningTable,
                     ex:Chair1,
                     ex:Chair2,
                     ex:Chair3,
                     ex:Chair4,
                     ex:CookiesBox,
                     ex:FruitBowl,
                     ex:Oranges,
                     ex:WaterBottle,
                     ex:GlassPitcher,
                     ex:WineGlass,
                     ex:Door .

ex:UpperCabinets a psr:Furniture .
ex:LowerCabinets a psr:Furniture .
ex:Countertop a psr:Furniture .
ex:Refrigerator a psr:Appliance .
ex:TallBlackCabinet a psr:Furniture .

ex:JamJar1 a psr:Object ;
    dul:hasLocation ex:JamJar1Location .
ex:JamJar1Location a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:StrawberryJelloBox a psr:Object ;
    dul:hasLocation ex:StrawberryJelloBoxLocation .
ex:StrawberryJelloBoxLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:SaltContainer a psr:Object ;
    dul:hasLocation ex:SaltContainerLocation .
ex:SaltContainerLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:WhitePlatesStack a psr:Object ;
    dul:hasLocation ex:WhitePlatesStackLocation .
ex:WhitePlatesStackLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:Sink a psr:Appliance ;
    dul:hasLocation ex:SinkLocation .
ex:SinkLocation a psr:CurrentLocation ;
    geo:sfWithin ex:Countertop .

ex:SmallMetalPot a psr:Object ;
    dul:hasLocation ex:SmallMetalPotLocation .
ex:SmallMetalPotLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Stove .

ex:VentHood a psr:Appliance .
ex:Stove a psr:Appliance .

ex:CerealBoxChucky a psr:Object ;
    dul:hasLocation ex:CerealBoxChuckyLocation .
ex:CerealBoxChuckyLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:CerealBoxRiceBubbles a psr:Object ;
    dul:hasLocation ex:CerealBoxRiceBubblesLocation .
ex:CerealBoxRiceBubblesLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:OrangeMug a psr:Object ;
    dul:hasLocation ex:OrangeMugLocation .
ex:OrangeMugLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:JamJar2 a psr:Object ;
    dul:hasLocation ex:JamJar2Location .
ex:JamJar2Location a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:DiningTable a psr:Furniture .

ex:Chair1 a psr:Furniture .
ex:Chair2 a psr:Furniture .
ex:Chair3 a psr:Furniture .
ex:Chair4 a psr:Furniture .

ex:CookiesBox a psr:Object ;
    dul:hasLocation ex:CookiesBoxLocation .
ex:CookiesBoxLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:FruitBowl a psr:Object ;
    dul:hasLocation ex:FruitBowlLocation .
ex:FruitBowlLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:Oranges a psr:Object ;
    dul:hasLocation ex:OrangesLocation .
ex:OrangesLocation a psr:CurrentLocation ;
    geo:sfWithin ex:FruitBowl .

ex:WaterBottle a psr:Object ;
    dul:hasLocation ex:WaterBottleLocation .
ex:WaterBottleLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:GlassPitcher a psr:Object ;
    dul:hasLocation ex:GlassPitcherLocation .
ex:GlassPitcherLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:WineGlass a psr:Object ;
    dul:hasLocation ex:WineGlassLocation .
ex:WineGlassLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:Door a psr:Furniture .