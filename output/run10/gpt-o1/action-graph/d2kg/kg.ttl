@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/data/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

#################################################################
# Instruction and Workflow
#################################################################

ex:instruction1 a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function. Prioritize actions according to logical task order, and perform each step atomically."^^xsd:string ;
    psr:hasWorkflow ex:workflow1 .

ex:workflow1 a psr:Workflow ;
    psr:hasAction ex:action1 ,
                  ex:action2 ,
                  ex:action3 ,
                  ex:action4 ,
                  ex:action5 ,
                  ex:action6 ,
                  ex:action7 ,
                  ex:action8 ,
                  ex:action9 ,
                  ex:action10 ,
                  ex:action11 ,
                  ex:action12 ,
                  ex:action13 ,
                  ex:action14 ,
                  ex:action15 ,
                  ex:action16 ,
                  ex:action17 ,
                  ex:action18 ,
                  ex:action19 ,
                  ex:action20 ,
                  ex:action21 ,
                  ex:action22 ,
                  ex:action23 ,
                  ex:action24 ,
                  ex:action25 ,
                  ex:action26 ,
                  ex:action27 ,
                  ex:action28 .

#################################################################
# Agent
#################################################################

ex:robot a dul:Agent .

#################################################################
# Components (Objects and Storage)
#################################################################

ex:pot a psr:Object .
ex:chuckyHarps1 a psr:Object .
ex:riceBubbles a psr:Object .
ex:plates a psr:Object .
ex:jamJar1 a psr:Object .
ex:saltContainer a psr:Object .
ex:jelloBox a psr:Object .

ex:lowerCabinet a psr:Furniture .
ex:pantry a psr:Furniture .
ex:overheadCabinet a psr:Furniture .
ex:fridge a psr:Appliance .

#################################################################
# Actions for Pot -> Lower Cabinet
#################################################################

ex:action1 a dul:Action ;
    soma:isPerformedBy ex:robot ;
    psr:actsOn ex:pot ;
    psr:isAffordedBy soma:PickingUp ;
    psr:precedes ex:action2 .

ex:action2 a dul:Action ;
    soma:isPerformedBy ex:robot ;
    psr:actsOn ex:lowerCabinet ;
    psr:isAffordedBy soma:Opening ;
    psr:precedes ex:action3 .

ex:action3 a dul:Action ;
    soma:isPerformedBy ex:robot ;
    psr:actsOn ex:pot ;
    psr:isAffordedBy soma:PuttingDown ;
    psr:precedes ex:action4 .

ex:action4 a dul:Action ;
    soma:isPerformedBy ex:robot ;
    psr:actsOn ex:lowerCabinet ;
    psr:isAffordedBy soma:Closing ;
    psr:precedes ex:action5 .

