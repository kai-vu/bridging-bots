@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/data/> .

ex:RestoreKitchenTask a psr:Instruction ;
    psr:hasWorkflow ex:RestoreKitchenWorkflow .

ex:RestoreKitchenWorkflow a psr:Workflow ;
    psr:hasAction ex:IdentifyMisplacedItemsAction ;
    psr:hasAction ex:PickUpMisplacedItemAction ;
    psr:hasAction ex:PutAwayMisplacedItemAction .

ex:IdentifyMisplacedItemsAction a dul:Action ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItems .

ex:PickUpMisplacedItemAction a soma:PickingUp ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItem .

ex:PutAwayMisplacedItemAction a soma:PuttingDown ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItem ;
    psr:follows ex:PickUpMisplacedItemAction .

ex:MisplacedItems a psr:Component ;
    rdfs:label "Misplaced items in the kitchen"@en .

ex:MisplacedItem a psr:Object ;
    rdfs:label "A misplaced item in the kitchen"@en .

ex:RobotAgent a dul:Agent ;
    rdfs:label "Robot agent performing the task"@en .

ex:KitchenCabinets a psr:Furniture ;
    rdfs:label "Dark grey kitchen cabinets"@en .

ex:MarbleCountertop a psr:Furniture ;
    rdfs:label "Light marble kitchen countertop"@en .

ex:DiningTable a psr:Furniture ;
    rdfs:label "Brown wooden dining table"@en .

ex:Refrigerator a psr:Appliance ;
    rdfs:label "White refrigerator"@en .

ex:IdentifyMisplacedItemsAction psr:precedes ex:PickUpMisplacedItemAction .
ex:PickUpMisplacedItemAction psr:precedes ex:PutAwayMisplacedItemAction .

ex:PutAwayMisplacedItemAction psr:isAffordedBy soma:PuttingDown .
ex:PickUpMisplacedItemAction psr:isAffordedBy soma:PickingUp .