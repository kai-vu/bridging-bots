@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/OntoBOT#> .

psr:Instruction_1 a psr:Instruction ;
    psr:hasWorkflow psr:Workflow_1 ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function. Prioritize actions according to logical task order, and perform each step atomically."@en .

psr:Workflow_1 a psr:Workflow ;
    psr:hasAction psr:Action_1, psr:Action_2, psr:Action_3, psr:Action_4, psr:Action_5, psr:Action_6, psr:Action_7, psr:Action_8 .

psr:Action_1 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:KitchenTable ;
    psr:isAffordedBy soma:PickingUp ;
    rdfs:label "Pick up Evian bottle"@en .

psr:Action_2 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:EvianBottle ;
    psr:isAffordedBy soma:PuttingDown ;
    rdfs:label "Put down Evian bottle on counter"@en ;
    dul:follows psr:Action_1 .

psr:Action_3 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:ChuckyBox, psr:RiceBubblesBox, psr:JelloBox, psr:AlpenSaltBox ;
    psr:isAffordedBy soma:PickingUp ;
    rdfs:label "Pick up boxes"@en ;
    dul:follows psr:Action_2 .

psr:Action_4 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:ChuckyBox, psr:RiceBubblesBox, psr:JelloBox, psr:AlpenSaltBox ;
    psr:isAffordedBy soma:PuttingDown ;
    rdfs:label "Put down boxes on counter"@en ;
    dul:follows psr:Action_3 .

psr:Action_5 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Crockery ;
    psr:isAffordedBy soma:PickingUp ;
    rdfs:label "Pick up crockery"@en ;
    dul:follows psr:Action_4 .

psr:Action_6 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Crockery ;
    psr:isAffordedBy soma:PuttingDown ;
    rdfs:label "Put down crockery in cabinet"@en ;
    dul:follows psr:Action_5 .

psr:Action_7 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Cups ;
    psr:isAffordedBy soma:PickingUp ;
    rdfs:label "Pick up cups"@en ;
    dul:follows psr:Action_6 .

psr:Action_8 a dul:Action ;
    soma:isPerformedBy psr:Robot_1 ;
    psr:actsOn psr:Cups ;
    psr:isAffordedBy soma:PuttingDown ;
    rdfs:label "Put down cups in cabinet"@en ;
    dul:follows psr:Action_7 .

psr:Robot_1 a dul:Agent .

psr:KitchenTable a psr:Furniture .
psr:EvianBottle a psr:Object .
psr:ChuckyBox a psr:Object .
psr:RiceBubblesBox a psr:Object .
psr:JelloBox a psr:Object .
psr:AlpenSaltBox a psr:Object .
psr:Crockery a psr:Object .
psr:Cups a psr:Object .