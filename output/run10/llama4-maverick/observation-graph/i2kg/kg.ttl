@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/OntoBOT#> .
@prefix ex: <http://example.org/data/> .

ex:Kitchen a psr:Environment ;
    dul:hasComponent ex:Cabinet1, ex:Cabinet2, ex:Refrigerator, ex:DiningTable, ex:Sink, ex:Stove .

ex:Cabinet1 a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation ex:CounterTop1 .

ex:Cabinet2 a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation ex:CounterTop2 .

ex:Refrigerator a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation ex:Floor1 .

ex:DiningTable a psr:Furniture ;
    psr:hasAffordance soma:Supporting ;
    dul:hasLocation ex:Floor1 .

ex:Sink a psr:Object ;
    psr:hasAffordance soma:Containing ;
    dul:hasLocation ex:CounterTop2 .

ex:Stove a psr:Object ;
    psr:hasAffordance soma:Heating ;
    dul:hasLocation ex:CounterTop2 .

ex:CounterTop1 a psr:Component ;
    dul:hasLocation ex:Cabinet1 .

ex:CounterTop2 a psr:Component ;
    dul:hasLocation ex:Cabinet2 .

ex:Floor1 a soma:Location .

ex:Jar1 a psr:Object ;
    dul:hasLocation ex:CounterTop1 ;
    psr:onTopOf ex:CounterTop1 .

ex:Box1 a psr:Object ;
    dul:hasLocation ex:CounterTop1 ;
    psr:onTopOf ex:CounterTop1 .

ex:RefrigeratorDoor a soma:Location ;
    geo:sfWithin ex:Refrigerator .

ex:Bottle1 a psr:Object ;
    dul:hasLocation ex:DiningTable ;
    psr:onTopOf ex:DiningTable .

ex:Glass1 a psr:Object ;
    dul:hasLocation ex:DiningTable ;
    psr:onTopOf ex:DiningTable .

ex:Book1 a psr:Object ;
    dul:hasLocation ex:DiningTable ;
    psr:onTopOf ex:DiningTable .

ex:Chair1 a psr:Furniture ;
    dul:hasLocation ex:Floor1 .

ex:Door1 a psr:Component ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation ex:Wall1 .

ex:Wall1 a psr:Component .

ex:Light1 a psr:Object ;
    dul:hasLocation ex:Ceiling1 .

ex:Ceiling1 a soma:Location .