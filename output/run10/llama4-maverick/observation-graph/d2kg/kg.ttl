@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/OntoBOT#> .
@prefix ex: <http://example.org/data/> .

ex:KitchenEnvironment a psr:Environment ;
    dul:hasComponent ex:KitchenCabinets, ex:Refrigerator, ex:MarbleCountertop, ex:WoodenFloor .

ex:KitchenCabinets a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation ex:CurrentKitchenCabinetsLocation .

ex:CurrentKitchenCabinetsLocation a psr:CurrentLocation ;
    psr:onTopOf ex:WoodenFloor ;
    geo:sfWithin ex:KitchenEnvironment ;
    geo:sfAdjacentTo ex:Refrigerator .

ex:Refrigerator a psr:Appliance ;
    psr:hasAffordance soma:Opening, soma:Closing ;
    dul:hasLocation ex:CurrentRefrigeratorLocation .

ex:CurrentRefrigeratorLocation a psr:CurrentLocation ;
    geo:sfWithin ex:KitchenEnvironment .

ex:MarbleCountertop a psr:Furniture ;
    psr:hasAffordance soma:Holding, soma:PuttingDown ;
    dul:hasLocation ex:CurrentMarbleCountertopLocation .

ex:CurrentMarbleCountertopLocation a psr:CurrentLocation ;
    psr:onTopOf ex:KitchenCabinets ;
    geo:sfWithin ex:KitchenEnvironment .

ex:WoodenFloor a psr:Object ;
    dul:hasLocation ex:CurrentWoodenFloorLocation .

ex:CurrentWoodenFloorLocation a psr:CurrentLocation ;
    geo:sfWithin ex:KitchenEnvironment .

ex:DiningRoomEnvironment a psr:Environment ;
    dul:hasComponent ex:DiningTable, ex:DiningChairs, ex:Lemons, ex:Laptop, ex:Book, ex:LightFixture .

ex:DiningTable a psr:Furniture ;
    psr:hasAffordance soma:Holding, soma:PuttingDown ;
    dul:hasLocation ex:CurrentDiningTableLocation .

ex:CurrentDiningTableLocation a psr:CurrentLocation ;
    geo:sfWithin ex:DiningRoomEnvironment .

ex:DiningChairs a psr:Furniture ;
    psr:hasAffordance soma:PickingUp, soma:PuttingDown ;
    dul:hasLocation ex:CurrentDiningChairsLocation .

ex:CurrentDiningChairsLocation a psr:CurrentLocation ;
    geo:sfWithin ex:DiningRoomEnvironment .

ex:Lemons a psr:Object ;
    dul:hasLocation ex:CurrentLemonsLocation .

ex:CurrentLemonsLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:Laptop a psr:Object ;
    dul:hasLocation ex:CurrentLaptopLocation .

ex:CurrentLaptopLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:Book a psr:Object ;
    dul:hasLocation ex:CurrentBookLocation .

ex:CurrentBookLocation a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:LightFixture a psr:Object ;
    dul:hasLocation ex:CurrentLightFixtureLocation .

ex:CurrentLightFixtureLocation a psr:CurrentLocation ;
    geo:sfWithin ex:DiningRoomEnvironment .

ex:Sink a psr:Object ;
    dul:hasLocation ex:CurrentSinkLocation .

ex:CurrentSinkLocation a psr:CurrentLocation ;
    psr:onTopOf ex:MarbleCountertop .

ex:Stove a psr:Appliance ;
    psr:hasAffordance soma:TurningOn, soma:TurningOff ;
    dul:hasLocation ex:CurrentStoveLocation .

ex:CurrentStoveLocation a psr:CurrentLocation ;
    psr:onTopOf ex:MarbleCountertop .

ex:Pot a psr:Object ;
    dul:hasLocation ex:CurrentPotLocation .

ex:CurrentPotLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Stove .

ex:Mirror a psr:Object ;
    dul:hasLocation ex:CurrentMirrorLocation .

ex:CurrentMirrorLocation a psr:CurrentLocation ;
    geo:sfWithin ex:KitchenEnvironment .

ex:KitchenExhaustHood a psr:Appliance ;
    dul:hasLocation ex:CurrentKitchenExhaustHoodLocation .

ex:CurrentKitchenExhaustHoodLocation a psr:CurrentLocation ;
    geo:sfWithin ex:KitchenEnvironment .