@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/onto-bot#> .
@prefix ex:     <http://example.org/data/> .

ex:Environment1 a psr:Environment .
ex:Kitchen a psr:Component ;
    dul:hasLocation ex:KitchenLocation .
ex:KitchenLocation a psr:Location ;
    psr:onTopOf ex:Floor ;
    geo:sfContains ex:Countertop1, ex:Countertop2 ;
    geo:sfWithin ex:Room1 .
ex:Floor a psr:Component ;
    dul:hasLocation ex:FloorLocation .
ex:FloorLocation a psr:Location .
ex:Countertop1 a psr:Component ;
    dul:hasLocation ex:Countertop1Location ;
    psr:hasAffordance soma:Grasping, soma:Pushing .
ex:Countertop1Location a psr:Location ;
    psr:onTopOf ex:Cabinet1 .
ex:Countertop2 a psr:Component ;
    dul:hasLocation ex:Countertop2Location ;
    psr:hasAffordance soma:Grasping, soma:Pushing .
ex:Countertop2Location a psr:Location ;
    psr:onTopOf ex:Cabinet2 .
ex:Cabinet1 a psr:Component ;
    dul:hasLocation ex:Cabinet1Location .
ex:Cabinet1Location a psr:Location .
ex:Cabinet2 a psr:Component ;
    dul:hasLocation ex:Cabinet2Location .
ex:Cabinet2Location a psr:Location .
ex:Refrigerator a psr:Appliance ;
    dul:hasLocation ex:RefrigeratorLocation ;
    psr:hasAffordance soma:Opening, soma:Closing .
ex:RefrigeratorLocation a psr:Location ;
    psr:onTopOf ex:Floor .
ex:RedMug a psr:Object ;
    dul:hasLocation ex:RedMugLocation ;
    psr:hasAffordance soma:Grasping, soma:Holding .
ex:RedMugLocation a psr:Location ;
    psr:onTopOf ex:Countertop1 .
ex:Bottle1 a psr:Object ;
    dul:hasLocation ex:Bottle1Location ;
    psr:hasAffordance soma:Grasping, soma:Holding .
ex:Bottle1Location a psr:Location ;
    psr:onTopOf ex:Countertop1 .
ex:Bottle2 a psr:Object ;
    dul:hasLocation ex:Bottle2Location ;
    psr:hasAffordance soma:Grasping, soma:Holding .
ex:Bottle2Location a psr:Location ;
    psr:onTopOf ex:Countertop1 .
ex:Room1 a psr:Component ;
    dul:hasLocation ex:Room1Location ;
    geo:sfContains ex:Kitchen, ex:Door .
ex:Room1Location a psr:Location .
ex:Door a psr:Component ;
    dul:hasLocation ex:DoorLocation ;
    psr:hasAffordance soma:Opening, soma:Closing .
ex:DoorLocation a psr:Location ;
    psr:onTopOf ex:Floor .
ex:Room2 a psr:Component ;
    dul:hasLocation ex:Room2Location ;
    geo:sfContains ex:Table, ex:Chairs, ex:Microwave, ex:Oven .
ex:Room2Location a psr:Location .
ex:Table a psr:Furniture ;
    dul:hasLocation ex:TableLocation .
ex:TableLocation a psr:Location ;
    psr:onTopOf ex:Floor .
ex:Chairs a psr:Furniture ;
    dul:hasLocation ex:ChairsLocation .
ex:ChairsLocation a psr:Location ;
    psr:onTopOf ex:Floor .
ex:Microwave a psr:Appliance ;
    dul:hasLocation ex:MicrowaveLocation ;
    psr:hasAffordance soma:Opening, soma:Closing .
ex:MicrowaveLocation a psr:Location ;
    psr:onTopOf ex:Countertop .
ex:Oven a psr:Appliance ;
    dul:hasLocation ex:OvenLocation ;
    psr:hasAffordance soma:Opening, soma:Closing .
ex:OvenLocation a psr:Location ;
    psr:onTopOf ex:Countertop .
ex:Environment1 dul:hasComponent ex:Kitchen, ex:Room2 .