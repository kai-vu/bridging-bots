@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix ex: <http://example.org/data/> .

ex:Kitchen a psr:Environment ;
    rdfs:label "Kitchen"@en ;
    dul:hasComponent ex:Countertop1, ex:Countertop2, ex:Refrigerator, ex:VentHood, ex:Cabinets, ex:Window, ex:Door .

ex:Countertop1 a psr:Component ;
    rdfs:label "Countertop 1"@en ;
    dul:hasLocation ex:Kitchen ;
    psr:hasAffordance soma:Grasping, soma:Pushing, soma:Pulling .

ex:Countertop2 a psr:Component ;
    rdfs:label "Countertop 2"@en ;
    dul:hasLocation ex:Kitchen ;
    psr:hasAffordance soma:Grasping, soma:Pushing, soma:Pulling .

ex:Refrigerator a psr:Appliance ;
    rdfs:label "Refrigerator"@en ;
    dul:hasLocation ex:Kitchen ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:VentHood a psr:Component ;
    rdfs:label "Vent Hood"@en ;
    dul:hasLocation ex:Kitchen .

ex:Cabinets a psr:Component ;
    rdfs:label "Cabinets"@en ;
    dul:hasLocation ex:Kitchen ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:Window a psr:Component ;
    rdfs:label "Window"@en ;
    dul:hasLocation ex:Kitchen .

ex:Door a psr:Component ;
    rdfs:label "Door"@en ;
    dul:hasLocation ex:Kitchen ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:OtherRoom a psr:Environment ;
    rdfs:label "Other Room"@en ;
    dul:hasComponent ex:Table, ex:Chairs, ex:Microwave, ex:Oven .

ex:Table a psr:Furniture ;
    rdfs:label "Table"@en ;
    dul:hasLocation ex:OtherRoom .

ex:Chairs a psr:Furniture ;
    rdfs:label "Chairs"@en ;
    dul:hasLocation ex:OtherRoom .

ex:Microwave a psr:Appliance ;
    rdfs:label "Microwave"@en ;
    dul:hasLocation ex:OtherRoom ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:Oven a psr:Appliance ;
    rdfs:label "Oven"@en ;
    dul:hasLocation ex:OtherRoom ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:RedMug a psr:Object ;
    rdfs:label "Red Mug"@en ;
    dul:hasLocation ex:Countertop1 ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:ClearBottle a psr:Object ;
    rdfs:label "Clear Bottle"@en ;
    dul:hasLocation ex:Countertop1 ;
    psr:hasAffordance soma:Grasping, soma:Holding .

ex:BrownBottle a psr:Object ;
    rdfs:label "Brown Bottle"@en ;
    dul:hasLocation ex:Countertop1 ;
    psr:hasAffordance soma:Grasping, soma:Holding .