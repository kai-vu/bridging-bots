@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-action#> .
@prefix ex:     <http://example.org/data/> .

ex:Kitchen a psr:Environment ;
    dul:hasComponent ex:Fridge ,
                     ex:Door ,
                     ex:Cabinet1 ,
                     ex:Sink ,
                     ex:RangeHood ,
                     ex:Stove ,
                     ex:Countertop ,
                     ex:Table ,
                     ex:Chair1 ,
                     ex:Chair2 ,
                     ex:Chair3 ,
                     ex:Chair4 ,
                     ex:Jam1 ,
                     ex:Jam2 ,
                     ex:Jello ,
                     ex:Salt ,
                     ex:CerealBox1 ,
                     ex:CerealBox2 ,
                     ex:Plates ,
                     ex:Pot ,
                     ex:Mug ,
                     ex:WaterBottle ,
                     ex:FruitBowl .

ex:Fridge a psr:Appliance ;
    psr:hasAffordance soma:Opening ,
                       soma:Closing .

ex:Door a psr:Object ;
    psr:hasAffordance soma:Opening ,
                       soma:Closing .

ex:Cabinet1 a psr:Furniture ;
    psr:hasAffordance soma:Opening ,
                       soma:Closing .

ex:Sink a psr:Appliance .

ex:RangeHood a psr:Appliance .

ex:Stove a psr:Appliance .

ex:Countertop a psr:Furniture .

ex:Table a psr:Furniture .

ex:Chair1 a psr:Furniture .
ex:Chair2 a psr:Furniture .
ex:Chair3 a psr:Furniture .
ex:Chair4 a psr:Furniture .

ex:Jam1 a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping .

ex:Jam2 a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping .

ex:Jello a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping .

ex:Salt a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping .

ex:CerealBox1 a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping .

ex:CerealBox2 a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping .

ex:Plates a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping .

ex:Pot a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping ,
                       soma:Holding .

ex:Mug a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping ,
                       soma:Holding .

ex:WaterBottle a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping ,
                       soma:Holding .

ex:FruitBowl a psr:Object ;
    psr:hasAffordance soma:PickingUp ,
                       soma:PuttingDown ,
                       soma:Grasping ,
                       soma:Holding .

ex:CountertopLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:StoveLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Stove .

ex:TableLocation a psr:CurrentLocation ;
    psr:onTopOf ex:Table .

ex:Jam1 dul:hasLocation ex:CountertopLocation .
ex:Jam2 dul:hasLocation ex:CountertopLocation .
ex:Jello dul:hasLocation ex:CountertopLocation .
ex:Salt dul:hasLocation ex:CountertopLocation .
ex:CerealBox1 dul:hasLocation ex:CountertopLocation .
ex:CerealBox2 dul:hasLocation ex:CountertopLocation .
ex:Plates dul:hasLocation ex:CountertopLocation .
ex:Mug dul:hasLocation ex:CountertopLocation .

ex:Pot dul:hasLocation ex:StoveLocation .

ex:FruitBowl dul:hasLocation ex:TableLocation .
ex:WaterBottle dul:hasLocation ex:TableLocation .