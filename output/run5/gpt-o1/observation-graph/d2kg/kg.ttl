@prefix ex: <http://example.org/data/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

ex:KitchenAndDining a psr:Environment ;
    dul:hasComponent ex:BlackCabinet,
                     ex:Countertop,
                     ex:Sink,
                     ex:Faucet,
                     ex:Cooktop,
                     ex:MetalPot,
                     ex:RangeHood,
                     ex:Mug,
                     ex:ChuckyHaros,
                     ex:RiceBubbles,
                     ex:StrawberryJam,
                     ex:WhitePlates,
                     ex:JelloBox,
                     ex:AlpenSaltContainer,
                     ex:PantryCabinet,
                     ex:Refrigerator,
                     ex:DiningTable,
                     ex:WhiteChair1,
                     ex:WhiteChair2,
                     ex:WhiteChair3,
                     ex:WhiteChair4,
                     ex:WhiteBowl,
                     ex:Oranges,
                     ex:EvianBottle,
                     ex:ShortbreadCookies,
                     ex:GlassPitcher,
                     ex:StemmedWineGlass,
                     ex:Door,
                     ex:CeilingLight,
                     ex:ParquetWoodFloor .

ex:BlackCabinet a psr:Furniture .
ex:Countertop a psr:Furniture .
ex:Sink a psr:Appliance .
ex:Faucet a psr:Appliance .
ex:Cooktop a psr:Appliance .
ex:MetalPot a psr:Object .
ex:RangeHood a psr:Appliance .
ex:Mug a psr:Object .
ex:ChuckyHaros a psr:Object .
ex:RiceBubbles a psr:Object .
ex:StrawberryJam a psr:Object .
ex:WhitePlates a psr:Object .
ex:JelloBox a psr:Object .
ex:AlpenSaltContainer a psr:Object .
ex:PantryCabinet a psr:Furniture .
ex:Refrigerator a psr:Appliance .
ex:DiningTable a psr:Furniture .
ex:WhiteChair1 a psr:Furniture .
ex:WhiteChair2 a psr:Furniture .
ex:WhiteChair3 a psr:Furniture .
ex:WhiteChair4 a psr:Furniture .
ex:WhiteBowl a psr:Object .
ex:Oranges a psr:Object .
ex:EvianBottle a psr:Object .
ex:ShortbreadCookies a psr:Object .
ex:GlassPitcher a psr:Object .
ex:StemmedWineGlass a psr:Object .
ex:Door a psr:Object .
ex:CeilingLight a psr:Object .
ex:ParquetWoodFloor a psr:Object .

ex:Sink dul:hasLocation ex:SinkLoc .
ex:SinkLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:Faucet dul:hasLocation ex:FaucetLoc .
ex:FaucetLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Sink .

ex:MetalPot dul:hasLocation ex:MetalPotLoc .
ex:MetalPotLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Cooktop .

ex:Mug dul:hasLocation ex:MugLoc .
ex:MugLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:ChuckyHaros dul:hasLocation ex:ChuckyHarosLoc .
ex:ChuckyHarosLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:RiceBubbles dul:hasLocation ex:RiceBubblesLoc .
ex:RiceBubblesLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:StrawberryJam dul:hasLocation ex:StrawberryJamLoc .
ex:StrawberryJamLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:WhitePlates dul:hasLocation ex:WhitePlatesLoc .
ex:WhitePlatesLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:JelloBox dul:hasLocation ex:JelloBoxLoc .
ex:JelloBoxLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:AlpenSaltContainer dul:hasLocation ex:AlpenSaltContainerLoc .
ex:AlpenSaltContainerLoc a psr:CurrentLocation ;
    psr:onTopOf ex:Countertop .

ex:WhiteBowl dul:hasLocation ex:WhiteBowlLoc .
ex:WhiteBowlLoc a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:Oranges dul:hasLocation ex:OrangesLoc .
ex:OrangesLoc a psr:CurrentLocation ;
    geo:sfWithin ex:WhiteBowl .

ex:EvianBottle dul:hasLocation ex:EvianBottleLoc .
ex:EvianBottleLoc a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:ShortbreadCookies dul:hasLocation ex:ShortbreadCookiesLoc .
ex:ShortbreadCookiesLoc a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:GlassPitcher dul:hasLocation ex:GlassPitcherLoc .
ex:GlassPitcherLoc a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .

ex:StemmedWineGlass dul:hasLocation ex:StemmedWineGlassLoc .
ex:StemmedWineGlassLoc a psr:CurrentLocation ;
    psr:onTopOf ex:DiningTable .