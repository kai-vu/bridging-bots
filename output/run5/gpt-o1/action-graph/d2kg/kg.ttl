@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/onto-bot#> .
@prefix ex: <http://example.org/data/> .

ex:Instruction_RestoreKitchen a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function. Prioritize actions according to logical task order, and perform each step atomically."^^xsd:string ;
    psr:hasWorkflow ex:Workflow_RestoreKitchen .

ex:Workflow_RestoreKitchen a psr:Workflow ;
    psr:hasAction ex:OpenPantryDoor,
                  ex:PickUpMug,
                  ex:PutDownMugInPantry,
                  ex:PickUpChuckyHaros,
                  ex:PutDownChuckyHarosInPantry,
                  ex:PickUpRiceBubbles,
                  ex:PutDownRiceBubblesInPantry,
                  ex:PickUpStrawberryJam,
                  ex:PutDownStrawberryJamInPantry,
                  ex:PickUpWhitePlates,
                  ex:PutDownWhitePlatesInPantry,
                  ex:PickUpJello,
                  ex:PutDownJelloInPantry,
                  ex:PickUpAlpenSalt,
                  ex:PutDownAlpenSaltInPantry,
                  ex:PickUpMetalPot,
                  ex:PutDownMetalPotInPantry,
                  ex:PickUpShortbreadCookies,
                  ex:PutDownShortbreadCookiesInPantry,
                  ex:PickUpGlassPitcher,
                  ex:PutDownGlassPitcherInPantry,
                  ex:PickUpWineGlass,
                  ex:PutDownWineGlassInPantry,
                  ex:ClosePantryDoor,
                  ex:OpenFridge,
                  ex:PickUpEvianWaterBottle,
                  ex:PutDownEvianWaterBottleInFridge,
                  ex:CloseFridge .

ex:Robot1 a dul:Agent .

ex:PantryCabinet a psr:Furniture .
ex:Fridge a psr:Appliance .

ex:Mug a psr:Object .
ex:ChuckyHaros a psr:Object .
ex:RiceBubbles a psr:Object .
ex:StrawberryJam a psr:Object .
ex:WhitePlates a psr:Object .
ex:Jello a psr:Object .
ex:AlpenSalt a psr:Object .
ex:MetalPot a psr:Object .
ex:ShortbreadCookies a psr:Object .
ex:GlassPitcher a psr:Object .
ex:WineGlass a psr:Object .
ex:EvianWaterBottle a psr:Object .

ex:OpenPantryDoor a dul:Action ;
    psr:actsOn ex:PantryCabinet ;
    psr:isAffordedBy soma:Opening ;
    soma:isPerformedBy ex:Robot1 ;
    psr:precedes ex:PickUpMug .

ex:PickUpMug a dul:Action ;
    psr:actsOn ex:Mug ;
    psr:isAffordedBy soma:PickingUp ;
    soma:isPerformedBy ex:Robot1 ;
    psr:precedes ex:PutDownMugInPantry .

ex:PutDownMugInPantry a dul:Action ;
    psr:actsOn ex:Mug ;
    psr:isAffordedBy soma:PuttingDown ;
    soma:isPerformedBy ex:Robot1 ;
    psr:precedes ex:PickUpChuckyHaros .

