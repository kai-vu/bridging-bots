@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix ex: <http://example.org/data/> .

ex:RestoreKitchenTask a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en ;
    psr:hasWorkflow ex:RestoreKitchenWorkflow .

ex:RestoreKitchenWorkflow a psr:Workflow ;
    psr:hasAction ex:IdentifyMisplacedItemsAction ;
    psr:hasAction ex:PickUpMisplacedItemsAction ;
    psr:hasAction ex:ReturnItemsToStorageLocationsAction .

ex:IdentifyMisplacedItemsAction a dul:Action ;
    psr:precedes ex:PickUpMisplacedItemsAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItems .

ex:PickUpMisplacedItemsAction a dul:Action ;
    psr:follows ex:IdentifyMisplacedItemsAction ;
    psr:precedes ex:ReturnItemsToStorageLocationsAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItems ;
    psr:isAffordedBy soma:PickingUp .

ex:ReturnItemsToStorageLocationsAction a dul:Action ;
    psr:follows ex:PickUpMisplacedItemsAction ;
    soma:isPerformedBy ex:RobotAgent ;
    psr:actsOn ex:MisplacedItems ;
    psr:isAffordedBy soma:PuttingDown .

ex:MisplacedItems a psr:Object ;
    rdfs:label "Misplaced items in the kitchen"@en .

ex:RobotAgent a dul:Agent ;
    rdfs:label "Robot agent performing the task"@en .

ex:StackOfPlates a psr:Object ;
    rdfs:label "Stack of white plates"@en .

ex:JarOfJam1 a psr:Object ;
    rdfs:label "Jar of jam on the counter"@en .

ex:BoxOfJello a psr:Object ;
    rdfs:label "Box of jello on the counter"@en .

ex:BoxOfSalt a psr:Object ;
    rdfs:label "Box of salt on the counter"@en .

ex:PlasticBottle a psr:Object ;
    rdfs:label "Plastic bottle on the dining table"@en .

ex:Carton a psr:Object ;
    rdfs:label "Carton on the dining table"@en .

ex:Mug a psr:Object ;
    rdfs:label "Mug on the dining table"@en .

ex:WineGlass a psr:Object ;
    rdfs:label "Wine glass on the dining table"@en .

ex:YellowCup a psr:Object ;
    rdfs:label "Yellow cup on the counter next to the sink"@en .

ex:BoxOfCrackers a psr:Object ;
    rdfs:label "Box of crackers on the counter next to the sink"@en .

ex:JarOfJam2 a psr:Object ;
    rdfs:label "Jar of jam on the counter next to the sink"@en .

ex:KitchenCabinets a psr:Furniture ;
    rdfs:label "Black and beige kitchen cabinets"@en .

ex:WhiteRefrigerator a psr:Appliance ;
    rdfs:label "White refrigerator next to the cabinets"@en .

ex:DiningTable a psr:Furniture ;
    rdfs:label "Wooden dining table"@en .

ex:Countertop a psr:Furniture ;
    rdfs:label "Beige marble countertop on the cabinets"@en .

