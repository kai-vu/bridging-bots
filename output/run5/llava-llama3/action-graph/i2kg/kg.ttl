@prefix psr: <http://psr.example.org/>
@prefix owl: <http://www.w3.org/2002/01/rdf-schema#>
@prefix dul: <http://www.dul.com/ontology#>
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>

# Define the prefixes
psr: psr.example.org/ ;
owl: rdf-schema.org/;
dul: <http://www.dul.com/ontology#>;
xsd: xmlschema.org/;

# Define the classes
owl:Instruction a psr:Instruction ;
psr:Instruction a owl:Class ;
psr:Workflow a psr:Instruction ;
dul:Action a psr:Instruction ;
dul:Agent a psr:Instruction ;
dul:Component a psr:Instruction ;
dul:Affordance a psr:Instruction ;
owl:Entity a psr:Instruction ;

# Define the properties
psr:hasWorkflow a psr:Instruction psr:Workflow ;
psr:hasAction a psr:Instruction dul:Action ;
psr:precedes a psr:Instruction dul:Action ;
psr:follows a psr:Instruction dul:Action ;
psr:isPerformedBy a psr:Instruction dul:Agent ;
psr:actsOn a psr:Instruction dul:Component ;
psr:isAffordedBy a psr:Instruction dul:Affordance ;

# Define the data properties
owl:hasNaturalLanguage a psr:Instruction xsd:string ;

# Assuming the robot must first identify all misplaced items, then categorize them by type and function, and finally return each item to its standard storage location based on their type and function.
# The sequence of actions below represents this process:
dul:Action a psr:Instruction ;
  dul:label "Identify misplaced items"@en .
  psr:precedes dul:Action dul:Action ;
  psr:follows dul:Action dul:Action ;
  psr:isPerformedBy dul:Agent ;
  psr:actsOn dul:Component ;
dul:Affordance a psr:Instruction dul:Action ;
  dul:label "Categorize items by type and function"@en .
  psr:precedes dul:Action dul:Action ;
  psr:follows dul:Action dul:Action ;
  psr:isAffordedBy dul:Agent ;
  psr:actsOn dul:Component ;
dul:Action a psr:Instruction ;
  dul:label "Return each item to its standard storage location"@en .
  psr:precedes dul:Action dul:Action ;
  psr:follows dul:Action dul:Action ;
  psr:isPerformedBy dul:Agent ;
  psr:actsOn dul:Component ;