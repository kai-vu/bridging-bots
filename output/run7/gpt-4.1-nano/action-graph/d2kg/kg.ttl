@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-action#> .
@prefix ex:     <http://example.org/data/> .

# Step 1: Identify all misplaced items
ex:IdentifyMisplacedItems a psr:Action ;
    rdfs:label "Identify all misplaced items in the kitchen."@en ;
    psr:actsOn ex:BoxOfJello, ex:CartonOfAlpenSalz, ex:RiceBubblesPackaging, ex:StrawberryJamJar, ex:PlasticWaterBottle, ex:Pitcher, ex:WineGlass ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

ex:PickUpBoxOfJello a psr:Action ;
    rdfs:label "Picking up the box of Jello pudding mix."@en ;
    psr:actsOn ex:BoxOfJello ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

ex:PickUpCartonOfAlpenSalz a psr:Action ;
    rdfs:label "Picking up the carton of Alpen Salz."@en ;
    psr:actsOn ex:CartonOfAlpenSalz ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

ex:PickUpRiceBubblesPackaging a psr:Action ;
    rdfs:label "Picking up the packaging of rice bubbles."@en ;
    psr:actsOn ex:RiceBubblesPackaging ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

ex:PickUpStrawberryJamJar a psr:Action ;
    rdfs:label "Picking up the small jar of strawberry jam."@en ;
    psr:actsOn ex:StrawberryJamJar ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

ex:PickUpPlasticWaterBottle a psr:Action ;
    rdfs:label "Picking up the plastic water bottle."@en ;
    psr:actsOn ex:PlasticWaterBottle ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

ex:PickUpPitcher a psr:Action ;
    rdfs:label "Picking up the pitcher."@en ;
    psr:actsOn ex:Pitcher ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

ex:PickUpWineGlass a psr:Action ;
    rdfs:label "Picking up the wine glass."@en ;
    psr:actsOn ex:WineGlass ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PickingUp .

# Step 2: Return items to their storage locations based on type and function

ex:ReturnBoxOfJelloToStorage a psr:Action ;
    rdfs:label "Returning the box of Jello pudding mix to its storage location."@en ;
    psr:actsOn ex:BoxOfJello ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PuttingDown .

ex:ReturnAlpenSalzToPantry a psr:Action ;
    rdfs:label "Returning the carton of Alpen Salz to pantry storage."@en ;
    psr:actsOn ex:CartonOfAlpenSalz ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PuttingDown .

ex:ReturnRiceBubblesPackagingToStorage a psr:Action ;
    rdfs:label "Returning the rice bubbles packaging to its storage."@en ;
    psr:actsOn ex:RiceBubblesPackaging ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PuttingDown .

ex:ReturnStrawberryJamJarToKitchenCabinet a psr:Action ;
    rdfs:label "Returning the strawberry jam jar to the kitchen cabinet."@en ;
    psr:actsOn ex:StrawberryJamJar ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PuttingDown .

ex:ReturnWaterBottleToRefrigerator a psr:Action ;
    rdfs:label "Returning the plastic water bottle to the refrigerator."@en ;
    psr:actsOn ex:PlasticWaterBottle ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PuttingDown .

ex:ReturnPitcherToStorage a psr:Action ;
    rdfs:label "Returning the pitcher to the storage cabinet."@en ;
    psr:actsOn ex:Pitcher ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PuttingDown .

ex:ReturnWineGlassToStorage a psr:Action ;
    rdfs:label "Returning the wine glass to the storage."@en ;
    psr:actsOn ex:WineGlass ;
    psr:isPerformedBy ex:RobotAgent ;
    psr:isAffordedBy soma:PuttingDown .

# Step 3: Ensure actions follow logical order and are atomic, as in sequence
# (Sequence implied by 'precedes' property)

ex:ActionSequence a owl:Collection ;
    rdf:first ex:IdentifyMisplacedItems ;
    rdf:rest _:rest1 .

_:rest1 rdf:first ex:PickUpBoxOfJello ;
    rdf:rest _:rest2 .

_:rest2 rdf:first ex:PickUpCartonOfAlpenSalz ;
    rdf:rest _:rest3 .

_:rest3 rdf:first ex:PickUpRiceBubblesPackaging ;
    rdf:rest _:rest4 .

_:rest4 rdf:first ex:PickUpStrawberryJamJar ;
    rdf:rest _:rest5 .

_:rest5 rdf:first ex:PickUpPlasticWaterBottle ;
    rdf:rest _:rest6 .

_:rest6 rdf:first ex:PickUpPitcher ;
    rdf:rest _:rest7 .

_:rest7 rdf:first ex:PickUpWineGlass ;
    rdf:rest nil .

ex:DefineTaskWorkflow a psr:Workflow ;
    rdfs:label "Workflow for restoring kitchen to organized state"@en ;
    psr:hasAction ex:IdentifyMisplacedItems, ex:PickUpBoxOfJello, ex:PickUpCartonOfAlpenSalz, ex:PickUpRiceBubblesPackaging, ex:PickUpStrawberryJamJar, ex:PickUpPlasticWaterBottle, ex:PickUpPitcher, ex:PickUpWineGlass, ex:ReturnBoxOfJelloToStorage, ex:ReturnAlpenSalzToPantry, ex:ReturnRiceBubblesPackagingToStorage, ex:ReturnStrawberryJamJarToKitchenCabinet, ex:ReturnWaterBottleToRefrigerator, ex:ReturnPitcherToStorage, ex:ReturnWineGlassToStorage .

ex:AssignWorkflowToInstruction a psr:Instruction ;
    rdfs:label "Assign the workflow to the instruction"@en ;
    psr:hasWorkflow ex:DefineTaskWorkflow .