@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/data/> .

ex:RestoreKitchenTask a psr:Instruction ;
    psr:hasNaturalLanguage "Restore the kitchen to an organized state by identifying all misplaced items and returning them to their standard storage locations based on their type and function."@en ;
    psr:hasWorkflow ex:RestoreKitchenWorkflow .

ex:RestoreKitchenWorkflow a psr:Workflow ;
    psr:hasAction ex:PickUpJELLOBox ;
    psr:hasAction ex:PickUpRaspberryJamJar ;
    psr:hasAction ex:PickUpAlpenSaltBox ;
    psr:hasAction ex:PickUpPlates ;
    psr:hasAction ex:PickUpPot ;
    psr:hasAction ex:PickUpWaterBottle ;
    psr:hasAction ex:PickUpWineGlass ;
    psr:hasAction ex:PickUpCoffeeMug ;
    psr:hasAction ex:PickUpPlate ;
    psr:hasAction ex:PickUpKnife ;
    psr:hasAction ex:PickUpSkonnbreadBox ;
    psr:hasAction ex:PutAwayJELLOBox ;
    psr:hasAction ex:PutAwayRaspberryJamJar ;
    psr:hasAction ex:PutAwayAlpenSaltBox ;
    psr:hasAction ex:PutAwayPlates ;
    psr:hasAction ex:PutAwayPot ;
    psr:hasAction ex:PutAwayWaterBottle ;
    psr:hasAction ex:PutAwayWineGlass ;
    psr:hasAction ex:PutAwayCoffeeMug ;
    psr:hasAction ex:PutAwayPlate ;
    psr:hasAction ex:PutAwayKnife ;
    psr:hasAction ex:PutAwaySkonnbreadBox .

