    psr:actsOn ex:CoffeeMug ;
    psr:isAffordedBy soma:PickingUp ;
    psr:precedes ex:PutAwayCoffeeMug .

ex:PutAwayCoffeeMug a dul:Action ;
    soma:isPerformedBy ex:Robot ;
    psr:actsOn ex:CoffeeMug ;
