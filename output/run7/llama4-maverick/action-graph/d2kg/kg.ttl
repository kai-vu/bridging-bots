ex:PickUpSkonnbreadBox a dul:Action ;
    soma:isPerformedBy ex:Robot ;
    psr:actsOn ex:SkonnbreadBox ;
    psr:isAffordedBy soma:PickingUp ;
    psr:precedes ex:PutAwaySkonnbreadBox .

ex:PutAwaySkonnbreadBox a dul:Action ;
