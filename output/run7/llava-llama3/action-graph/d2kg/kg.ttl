    psr:follows ex:action14 ;
    soma:isPerformedBy ex:robot ;
    psr:actsOn ex:diningTable ;
    psr:isAffordedBy soma:Grasping .

ex:action16 a dul:Action ;
    psr:follows ex:action15 ;
    soma:isPerformedBy ex:robot ;
    psr:actsOn ex:diningTable ;
    psr:isAffordedBy soma:Grasping .

ex:robot a dul:Agent .

ex:kitchen a psr:Component .
ex:sink a psr:Component .
ex:cabinet a psr:Component .
ex:coffeeMaker a psr:Component .
ex:refrigerator a psr:Component .
ex:smallBox a psr:Component .
ex:table a psr:Component .
ex:vase a psr:Component .
ex:oven a psr:Component .
ex:boxOnShelf a psr:Component .
ex:diningTable a psr:Component .