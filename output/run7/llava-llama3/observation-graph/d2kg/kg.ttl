@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix geo:    <http://www.opengis.net/ont/geosparql#> .
@prefix soma:   <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul:    <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr:    <https://w3id.org/psr-action#> .
@prefix ex:     <http://example.org/data/> .

ex:kitchen a psr:Environment ;
    dul:hasComponent ex:sink, ex:coffeeMaker, ex:cabinets, ex:refrigerator, ex:smallBox, ex:table, ex:vase, ex:chair, ex:oven, ex:shelf, ex:box .

ex:sink a psr:Component ;
    dul:hasLocation ex:sinkLocation ;
    psr:hasAffordance soma:Closing, soma:Opening .

ex:sinkLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen .

ex:coffeeMaker a psr:Component ;
    dul:hasLocation ex:coffeeMakerLocation ;
    psr:hasAffordance soma:Delivering .

ex:coffeeMakerLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen ;
    psr:onTopOf ex:countertop .

ex:countertop a psr:Component ;
    dul:hasLocation ex:countertopLocation .

ex:countertopLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen .

ex:cabinets a psr:Component ;
    dul:hasLocation ex:cabinetsLocation .

ex:cabinetsLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen ;
    geo:sfAbove ex:sinkLocation .

ex:refrigerator a psr:Component ;
    dul:hasLocation ex:refrigeratorLocation ;
    psr:hasAffordance soma:Opening, soma:Closing .

ex:refrigeratorLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen ;
    psr:onTopOf ex:floor .

ex:floor a psr:Component ;
    dul:hasLocation ex:floorLocation .

ex:floorLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen .

ex:smallBox a psr:Component ;
    dul:hasLocation ex:smallBoxLocation .

ex:smallBoxLocation a psr:CurrentLocation ;
    geo:sfOnTopOf ex:refrigerator .

ex:table a psr:Component ;
    dul:hasLocation ex:tableLocation .

ex:tableLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen .

ex:vase a psr:Component ;
    dul:hasLocation ex:vaseLocation .

ex:vaseLocation a psr:CurrentLocation ;
    geo:sfOnTopOf ex:table .

ex:chair a psr:Component ;
    dul:hasLocation ex:chairLocation .

ex:chairLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen ;
    geo:sfNear ex:table .

ex:oven a psr:Component ;
    dul:hasLocation ex:ovenLocation ;
    psr:hasAffordance soma:Delivering .

ex:ovenLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen ;
    geo:sfOpposite ex:sinkLocation .

ex:shelf a psr:Component ;
    dul:hasLocation ex:shelfLocation .

ex:shelfLocation a psr:CurrentLocation ;
    geo:sfWithin ex:kitchen ;
    geo:sfAbove ex:oven .

ex:box a psr:Component ;
    dul:hasLocation ex:boxLocation .

ex:boxLocation a psr:CurrentLocation ;
    geo:sfOnTopOf ex:shelf .