@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix soma: <http://www.ease-crc.org/ont/SOMA.owl#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix psr: <https://w3id.org/psr-action#> .
@prefix ex: <http://example.org/data/> .

ex:KitchenEnv a psr:Environment ;
   dul:hasComponent ex:Fridge,
                    ex:Countertop,
                    ex:WallCabinet,
                    ex:LowerCabinet,
                    ex:DiningTable,
                    ex:DiningChair1,
                    ex:DiningChair2,
                    ex:DiningChair3,
                    ex:DiningChair4,
                    ex:JelloBox,
                    ex:StrawberryJam,
                    ex:AlpenSalt,
                    ex:ChuckyCereal,
                    ex:RiceBubbles,
                    ex:PlateStack,
                    ex:Pot,
                    ex:Mug,
                    ex:EvianBottle,
                    ex:FruitBowl,
                    ex:Pitcher,
                    ex:Glass,
                    ex:TeaBox .

ex:Fridge a psr:Appliance ;
   psr:hasAffordance soma:Opening,
                      soma:Closing .

ex:Countertop a psr:Furniture .

ex:WallCabinet a psr:Furniture ;
   psr:hasAffordance soma:Opening,
                      soma:Closing .

ex:LowerCabinet a psr:Furniture ;
   psr:hasAffordance soma:Opening,
                      soma:Closing .

ex:DiningTable a psr:Furniture .

ex:DiningChair1 a psr:Furniture .
ex:DiningChair2 a psr:Furniture .
ex:DiningChair3 a psr:Furniture .
ex:DiningChair4 a psr:Furniture .

ex:JelloBox a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:StrawberryJam a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:AlpenSalt a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:ChuckyCereal a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:RiceBubbles a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:PlateStack a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:Pot a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:Mug a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:EvianBottle a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:FruitBowl a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:Pitcher a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:Glass a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:TeaBox a psr:Object ;
   psr:hasAffordance soma:PickingUp,
                      soma:PuttingDown .

ex:CountertopSurface a psr:CurrentLocation ;
   psr:onTopOf ex:Countertop .

ex:TableSurface a psr:CurrentLocation ;
   psr:onTopOf ex:DiningTable .

ex:JelloBox dul:hasLocation ex:CountertopSurface .
ex:StrawberryJam dul:hasLocation ex:CountertopSurface .
ex:AlpenSalt dul:hasLocation ex:CountertopSurface .
ex:ChuckyCereal dul:hasLocation ex:CountertopSurface .
ex:RiceBubbles dul:hasLocation ex:CountertopSurface .
ex:PlateStack dul:hasLocation ex:CountertopSurface .
ex:Pot dul:hasLocation ex:CountertopSurface .
ex:Mug dul:hasLocation ex:CountertopSurface .

ex:EvianBottle dul:hasLocation ex:TableSurface .
ex:FruitBowl dul:hasLocation ex:TableSurface .
ex:Pitcher dul:hasLocation ex:TableSurface .
ex:Glass dul:hasLocation ex:TableSurface .
ex:TeaBox dul:hasLocation ex:TableSurface .